    "mqtt": "^2.17.0",
    "multer": "^1.3.1",
    "object-hash": "^1.3.0",
    "rxjs": "^6.2.1",
    "socket.io": "~2.1.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.1",
    "@types/express": "^4.16.0",
    "@types/multer": "^1.3.7",
    "@types/node": "10.0.x",
    "@types/object-hash": "^1.2.0",
    "@types/socket.io": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "nodemon": "~1.11.0",
    "npm-run-all": "~4.1.1",
    "rimraf": "~2.6.2",
